<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.shofuku.accsystem.domain.inventory.UnlistedItem"		table="t300_unlisted_items">
	
		<id name="id" type="int" column="id">
         <generator class="native"/>
        </id>
        
		<property name="itemCode" 						type="string" 	column="ITEM_CODE"></property>
		<property name="classification" 				type="string" 	column="CLASSIFICATION" />
		<property name="description" 				type="string" 	column="DESCRIPTION" />
		<property name="uom" 			type="string" 	column="UOM" />
		<property name="template" 				type="string" 	column="TEMPLATE" />

	</class>
</hibernate-mapping>