<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shofuku.accsystem.domain.inventory.Warehouse"
		table="t310_warehouse">

		<id name="id" 						type="int" 	column="ID">
		<generator class="increment"/>
		</id>
		<property name="locationCode" 				type="string" 	column="LOCATION_CODE" />
		<property name="itemCode" 				type="string" 	column="ITEM_CODE" />
		<property name="quantityPerRecord" 			type="double" 	column="QUANTITY_PER_RECORD" />
		<property name="quantityPerPhysicalCount" 	type="double"	column="QUANTITY_PER_PHYSICAL" />
		
	</class>
</hibernate-mapping>