<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shofuku.accsystem.domain.security.Role"
		table="t802_role">

		<id name="roleId" type="int" column="ROLE_ID" />
		<property name="roleName" type="string" column="ROLE_NAME" />
		<property name="roleAccessString" type="string" column="ROLE_ACCESS_STRING" />
		
	</class>
</hibernate-mapping>