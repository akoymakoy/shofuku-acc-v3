<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shofuku.accsystem.domain.suppliers.Supplier"
		table="t101_suppliers">
		<id name="supplierId" type="string" column="SUPPLIER_ID" ></id>
		<property name="supplierName" type="string" column="SUPPLIER_NAME" />
		<property name="supplierStatus" type="string" column="SUPPLIER_STATUS" />
		<property name="contactName" type="string" column="CONTACT_NAME" />
		<property name="contactTitle" type="string" column="CONTACT_TITLE" />
		<property name="companyAddress" type="string" column="COMPANY_ADDRESS" />
		<property name="phoneNumber" type="string" column="PHONE_NUMBER" />
		<property name="faxNumber" type="string" column="FAX_NUMBER" />
		<property name="mobileNumber" type="string" column="MOBILE_NUMBER" />
		<property name="emailAddress" type="string" column="EMAIL_ADDRESS" />
		<property name="website" type="string" column="WEBSITE" />
		<property name="paymentTerm" type="string" column="PAYMENT_TERM" />
		<property name="tin" type="string" column="TIN" />
		
		<property name="location" type="string" column="LOCATION" />
	</class>
</hibernate-mapping>